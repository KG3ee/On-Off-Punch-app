{
  "name": "@modern-punch/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ../../node_modules/tsx/dist/cli.mjs watch src/main.ts",
    "build": "node ../../node_modules/typescript/bin/tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "prisma:generate": "node ../../node_modules/prisma/build/index.js generate --schema prisma/schema.prisma",
    "prisma:migrate": "node ../../node_modules/prisma/build/index.js migrate dev --schema prisma/schema.prisma",
    "prisma:studio": "node ../../node_modules/prisma/build/index.js studio --schema prisma/schema.prisma",
    "seed": "node ../../node_modules/tsx/dist/cli.mjs prisma/seed.ts"
  },
  "dependencies": {
    "@modern-punch/core": "file:../../packages/core",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.4.1",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/node": "^22.10.5",
    "prisma": "^6.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
